@model IEnumerable<CitizenshipView>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Person
            </th>
            <th>
                City
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PersonName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CityName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Citizenship.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Citizenship.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Citizenship.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
